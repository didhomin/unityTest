"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wdfall.dao.ItemDao">
    
    <select id="itemList" resultType="itemVO">
        SELECT item_seq itemSeq,item_name itemName,item_price itemPrice,item_image_path itemImagePath
        FROM item
        
    </select>
    
